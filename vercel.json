{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "functions": {
    "api/index.py": {
      "excludeFiles": "**/{venv,env,.venv,__pycache__,.git,.pytest_cache,*.pyc,*.pyo,*.pyd,.Python,*.so,*.egg,*.egg-info,app/scrapers,app/import,tests,*.md,requirements-scrapers.txt}/**"
    }
  }
}

